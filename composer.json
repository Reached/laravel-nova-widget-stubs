{
    "name": "reached/laravel-nova-widget-stubs",
    "description": "",
    "keywords": [
        "reached",
        "laravel-nova-widget-stubs"
    ],
    "homepage": "https://github.com/reached/laravel-nova-widget-stubs",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Casper SÃ¸rensen",
            "email": "casper.aarby.sorensen@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Reached\\LaravelNovaWidgetStubs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Reached\\LaravelNovaWidgetStubs\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Reached\\LaravelNovaWidgetStubs\\LaravelNovaWidgetStubsServiceProvider"
            ],
            "aliases": {
                "LaravelNovaWidgetStubs": "Reached\\LaravelNovaWidgetStubs\\LaravelNovaWidgetStubsFacade"
            }
        }
    }
}
